<%- include('partials/header') %>

<div>
    <h3><%= results[0].title %></h3>
    <h4><%= results[0].description %></h4>


    <% for (const tag of results) { %>
        <ul>
            <li>
                <a href="/tags/<%= tag.id %>" class="btn btn-lg btn-block btn-primary text-white"><%= tag.name %></a>
            </li>
        </ul>

    <% } %>
</div>

<%- include('partials/footer') %>